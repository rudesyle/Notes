<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ThyNotebook.Web.Models" assembly="ThyNotebook.Web">
  <class name="Note" table="notes" dynamic-update="true">
    <id name="NoteId" column="NoteId" type="int" unsaved-value="0">
      <generator class="native" />
    </id>
    <property name="Name" type="string" length="200" not-null="true" />
    <property name="Content" type="string" />
    <property name="CreateDate" type="Timestamp" length="10000000" />
    <property name="NotebookId" type="int" insert="true" update="false" />
  </class>
</hibernate-mapping>